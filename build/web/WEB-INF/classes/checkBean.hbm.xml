<hibernate-mapping>
    <class name="biomart.Bean.CheckBean" table="Check_details">
        <id name="id" type="int" column="Id">
            <generator class="increment"/>
        </id>
        <property name="receiptNo" type="string" column="Receipt_no"/>
        <property name="submitDate" type="string" column="Submit_date"/>
        <property name="checkNo" type="string" column="Check_no"/>
         <property name="amount" type="float" column="amount"/>
         <property name="status" type="string" column="Status"/>
    </class>
</hibernate-mapping>

