<hibernate-mapping>
  <class name="biomart.Bean.PaymentDetailsBean" table="Payment_details">
    <id column="Id" name="id" type="int">
      <generator class="increment"/>
    </id>
    <property column="Receipt_no" name="receiptNo" type="string"/>
    <property column="Amount_paid" name="amountPaid" type="float"/>
    <property column="Payment_date" name="paymentDate" type="date"/>
  </class>
</hibernate-mapping>
