<hibernate-mapping>
  <class name="biomart.Bean.LoginBean" table="Login_tb">
    <id column="User_id" name="userId" type="string">
      <generator class="assigned"/>
    </id>
    <property column="Password" name="password" type="string"/>
    <property column="Type" name="type" type="string"/>
    <property column="Login_time" name="loginTime" type="date"/>
    <property column="Logout_time" name="logoutTime" type="date"/>
    <property column="Status" name="status" type="string"/>
  </class>
</hibernate-mapping>
